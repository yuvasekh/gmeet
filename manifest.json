{
    "manifest_version": 3,
    "name": "Meet Translator",
    "version": "1.0",
    "permissions": [
      "tabCapture",
      "activeTab",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://meet.google.com/siv-udyg-enj?authuser=0"
    ]
  }